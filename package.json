    "nodejs",
    "toolkit",
    "utilities",
    "filesystem",
    "network",
    "archive",
    "git",
    "npm",
    "ssl",
    "process",
    "logger",
    "config",
    "cli",
    "inquirer",
    "notification",
    "performance",
    "database"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./filesystem": {
      "types": "./dist/filesystem/index.d.ts",
      "import": "./dist/filesystem/index.js",
      "require": "./dist/filesystem/index.cjs"
    },
