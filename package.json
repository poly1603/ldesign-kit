    "./iconfont": {
      "types": "./dist/iconfont/index.d.ts",
      "import": "./dist/iconfont/index.js",
      "require": "./dist/iconfont/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --testPathPattern=integration",
    "test:unit": "vitest run tests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "clean": "rimraf dist es lib types coverage .nyc_output",
    "size-check": "size-limit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:typedoc": "typedoc",
    "demo:scaffold": "node examples/scaffold-demo.js",
    "demo:console": "node examples/console-ui-demo.js",
    "demo:vite-builder": "node examples/builder/vite-builder-demo.js",
    "demo:rollup-builder": "node examples/builder/rollup-builder-demo.js",
    "demo:builder-comprehensive": "node examples/builder/comprehensive-demo.js",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
