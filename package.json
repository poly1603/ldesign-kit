    "@types/cli-progress": "^3.11.6",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "@types/tar": "^6.1.13",
    "@types/ws": "^8.5.10",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^9.18.0",
    "playwright": "^1.40.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.6.0",
