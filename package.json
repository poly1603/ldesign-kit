    "size-limit": "^11.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.7.3",
    "vitepress": "^1.6.4",
    "vitest": "^1.6.1",
    "@antfu/eslint-config": "^6.0.0"
