      "types": "./dist/network/index.d.ts",
      "import": "./dist/network/index.js",
      "require": "./dist/network/index.cjs"
    },
    "./archive": {
      "types": "./dist/archive/index.d.ts",
      "import": "./dist/archive/index.js",
      "require": "./dist/archive/index.cjs"
    },
    "./git": {
      "types": "./dist/git/index.d.ts",
      "import": "./dist/git/index.js",
      "require": "./dist/git/index.cjs"
    },
    "./package": {
      "types": "./dist/package/index.d.ts",
      "import": "./dist/package/index.js",
      "require": "./dist/package/index.cjs"
    },
    "./ssl": {
      "types": "./dist/ssl/index.d.ts",
      "import": "./dist/ssl/index.js",
      "require": "./dist/ssl/index.cjs"
    },
    "./process": {
      "types": "./dist/process/index.d.ts",
      "import": "./dist/process/index.js",
      "require": "./dist/process/index.cjs"
    },
    "./logger": {
      "types": "./dist/logger/index.d.ts",
      "import": "./dist/logger/index.js",
      "require": "./dist/logger/index.cjs"
